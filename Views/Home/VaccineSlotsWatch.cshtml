@model Settings;

<div class="row mb-4">
    <div class="col text-center">
        <h4>Checking for vaccine slots every 30 minutes</h4>
    </div>
</div>
<div class="row">
    <div class="col">
        <div class="d-flex justify-content-center">
            <div class="spinner-border" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
    </div>
</div>


@{
    Settings.VaccineSlotsWatch(Model);
}
